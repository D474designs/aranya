{
	// The daemon's name.
	"name": "name",
	
	// The daemon's working directory.
	//
	// Must already exist.
	"work_dir": "./out",

	// Used to receive API requests from the user library client.
	"uds_api_path": "./out/uds_api.sock",

	// The path where the daemon should write its PID file.
	"pid_file": "./out/daemon.pid"
}
